{
	"$schema": "../gen/schemas/desktop-schema.json",
	"identifier": "default",
	"description": "Capability for the main window",
	"windows": [
		"main"
	],
	"permissions": [
		"core:default",
		"opener:default",
		"fs:default",
		"fs:read-all",
		"fs:allow-read",
		"fs:allow-read-dir",
		"fs:allow-read-file",
		"fs:allow-write",
		"fs:allow-write-file",
		"fs:allow-create",
		"fs:allow-remove",
		"fs:allow-rename",
		"fs:scope-home-recursive"
	]
}